<div class="hd posts">
  
    <div class="post-title">
        <p>
            <%= page.title %>
        </p>
        <hr>
    </div>
    <div class="post-content">
        <%- page.content %>
    </div>

    <% if (theme.valine.enable && page.comments) { %>
        <hr class="fhr">
        <div id="vcomments"></div>
    <% } %>
</div>
